    "remix-development-tools": "^4.4.1",
    "remix-flat-routes": "^0.6.5",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240903.0",
    "@eslint/compat": "^1.1.1",
    "@remix-run/dev": "^2.11.1",
